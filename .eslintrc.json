{  "ecmaFeatures": {
    "globalReturn": true,
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "document": false,
    "escape": false,
    "navigator": false,
    "unescape": false,
    "window": false,
    "describe": true,
    "before": true,
    "it": true,
    "expect": true,
    "sinon": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
  ],
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "no-console": 0,
    "space-before-function-paren": ["error", "never"],
    "no-multi-spaces": ["error", { "exceptions": { "ImportDeclaration": true } }],
    "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true } }],
    "func-names": 0
  }
}
